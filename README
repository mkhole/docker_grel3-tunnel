# Start a new Docker container with the following command
docker run \
    --name grel3-tunnel \
    --env-file ./env.file \
    -v /lib/modules:/lib/modules:ro \
    -d --privileged \
    grel3-tunnel:0125


# Attach a living container
docker exec -it grel3-tunnel ash

# Retrieve contianer details
docker logs grel3-tunnel 
